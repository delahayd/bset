# equal_range_12.zf

include "bset.zf".

goal forall (a b c d : type) (S : set a) (T : set b) (U : set c) (V : set d) (P : set (tup a b)) (Q : set (tup c d)).
  mem (set (tup a b)) P (relation a b S T) && mem (set (tup c d)) Q (relation c d U V) => ran (tup a c) (tup b d) (parallel_product a b c d P Q) = times b d (ran a b P) (ran c d Q).