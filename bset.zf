#relation
rewrite
   forall (a b : type) (U : set a) (V : set b) (R: set (tup a b)).
     mem (set (tup a b)) R (relation a b U V) <=>
      (forall (T: tup a b).
        mem (tup a b) T R => mem a (fst T) U && mem b (snd T) V).
