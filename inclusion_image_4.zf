# inclusion_image_4.zf

include "bset.zf".

goal forall (a b : type) (S U : set a) (T : set b) (P Q : set (tup a b)).
  mem (set (tup a b)) P (relation a b S T) && mem (set (tup a b)) Q (relation a b S T) && subset a U S => subset b (diff b (image a b P U) (image a b Q U)) (image a b (diff (tup a b) P Q) U).
