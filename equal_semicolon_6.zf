# equal_semicolon_6.zf

include "bset.zf".

goal forall (a b c : type) (S : set a) (T V : set b) (U : set c) (R : set (tup a b)) (P : set (tup b c)).
  mem (set (tup a b)) R (relation a b S T) && subset b V T && mem (set (tup b c)) P (relation b c T U) => semicolon a b c R (domain_restriction b c V P) = semicolon a b c (range_restriction a b R V) P.