# equal_semicolon_28.zf

include "bset.zf".

goal forall (a b c : type) (S : set a) (T : set b) (U : set c) (x : a) (y : b) (R : set (tup b c)).
  mem a x S && mem b y T && mem (set (tup b c)) R (relation b c T U) => semicolon a b c (singleton (tup a b) (pair a b x y)) R = times a c (singleton a x) (image b c R (singleton b y)).