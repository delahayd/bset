#equal_image_10.zf

include "bset.zf".

goal forall (a b c d : type) (S U : set a) (T : set b) (W V : set c) (Z : set d) (P : set (tup a b)) (Q : set (tup c d)).
  mem (set (tup a b)) P (relation a b S T) && mem (set (tup c d)) Q (relation c d W Z) && subset a U S && subset c V W => image (tup a c) (tup b d) (parallel_product a b c d P Q) (times a c U V) = times b d (image a b P U) (image c d Q V).