# equal_substraction_10.zf

include "bset.zf".

goal forall (a b c : type) (S U : set a) (T: set b) (V : set c) (P : set (tup a b)) (Q : set (tup a c)).
  subset a U S && mem (set (tup a b)) P (relation a b S T) && mem (set (tup a c)) Q (relation a c S V) => (direct_product a b c (domain_substraction a b U P) (domain_substraction a c U Q) = domain_substraction a (tup b c) U (direct_product a b c P Q)).
