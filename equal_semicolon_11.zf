# equal_semicolon_11.zf

include "bset.zf".

goal forall (a b c : type) (S : set a) (T : set b) (U : set c) (R : set (tup a b)) (P Q: set (tup b c)).
  mem (set (tup a b)) R (relation a b S T) && mem (set (tup b c)) P (relation b c T U) && mem (set (tup b c)) Q (relation b c T U) => semicolon a b c R (union (tup b c) P Q) = union (tup a c) (semicolon a b c R P) (semicolon a b c R Q).