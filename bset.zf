#semicolon
rewrite
  forall (a b c : type) (P : set (tup a b)) (Q : set (tup b c)) (T : (tup a c)).
    mem (tup a c) T (semicolon a b c P Q) <=>
    (exists (b1 : b). mem (tup a b) (pair a b (fst T) b1) P && mem (tup b c) (pair b c b1 (snd T)) Q).


