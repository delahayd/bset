# equal_restriction_12.zf

include "bset.zf".

goal forall (a b : type) (S U : set a) (T : set b) (P Q : set (tup a b)).
  subset a U S && mem (set (tup a b)) P (relation a b S T) && mem (set (tup a b)) Q (relation a b S T) => domain_restriction a b U (union (tup a b) P Q) = union (tup a b) (domain_restriction a b U P) (domain_restriction a b U Q).