# monot_restriction_1.zf

include "bset.zf".

goal forall (a b : type) (S U V: set a) (T : set b) (P R : set (tup a b)).
  mem (set (tup a b)) P (relation a b S T) && subset a V S => ((subset a U V && subset (tup a b) R P) => subset (tup a b) (domain_restriction a b U R) (domain_restriction a b V P)).