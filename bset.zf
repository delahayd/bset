#direct_product
rewrite
  forall (a b c : type) (F : set (tup a b)) (G : set (tup a c)) (V : tup a (tup b c)).
    mem (tup a (tup b c)) V (direct_product a b c F G)
      <=> mem (tup a b) (pair (fst V) (fst (snd V))) F && mem (tup a c) (pair (fst V) (snd (snd V))) G.

