# equal_semicolon_2.zf

include "bset.zf".

goal forall (a b c: type) (S : set a) (T : set b) (U : set c) (R : set (tup b c)) (P Q : set (tup a b)).
  mem (set (tup a b)) P (relation a b S T) && mem (set (tup a b)) Q (relation a b S T) && mem (set (tup c b)) (inverse b c R) (plmngt c b U T) && semicolon a b c P R = semicolon a b c Q R => P = Q.