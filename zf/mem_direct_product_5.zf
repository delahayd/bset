#mem_direct_product_5.zf

include "bset.zf".

goal forall (a b c : type) (S : set a) (T : set b) (U : set c) (F : set (tup a b)) (G : set (tup a c)).
  mem (set (tup a b)) F (gtmngt a b S T) && mem (set (tup a c)) G (gtmngt a c S U) => mem (set (tup a (tup b c))) (direct_product a b c F G) (gtmngt a (tup b c) S (times b c T U)).
