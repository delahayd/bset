# monot_restriction_2.zf

include "bset.zf".

goal forall (a b : type) (S : set a) (T U V: set b) (P R : set (tup a b)).
  mem (set (tup a b)) P (relation a b S T) && subset b V T => ((subset (tup a b) R P && subset b U V) => subset (tup a b) (range_restriction a b R U) (range_restriction a b P V)).