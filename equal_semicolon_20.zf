# equal_semicolon_20.zf

include "bset.zf".

goal forall (a b : type) (S U : set a) (T : set b) (P : set (tup a b)) (R : set (tup b a)).
   subset a U S && mem (set (tup a b)) P (relation a b S T) && mem (set (tup b a)) R (relation b a T U) => semicolon a b a (domain_restriction a b U P) R = domain_restriction a a U (semicolon a b a P R).