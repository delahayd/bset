# equal_restriction_10.zf

include "bset.zf".

goal forall (a b c : type) (S U : set a) (T : set b) (V : set c) (P : set (tup a b)) (Q : set (tup a c)).
  subset a U S && mem (set (tup a b)) P (relation a b S T) && mem (set (tup a c)) Q (relation a c S V) => domain_restriction a (tup b c) U (direct_product a b c P Q) = direct_product a b c (domain_restriction a b U P) (domain_restriction a c U Q).